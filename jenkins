pipeline {
    agent none

    stages {
	agent slave1
        stage('BUILD') {
            steps {
		sh '''
			#!/bin/bash
			cd /var/lib/jenkins/workspace/c-project-test
			git pull  https://github.com/naveenas1414/c-project-test.git
			if [ $? -ne 0 ];then
			git clone https://github.com/naveenas1414/c-project-test.git
			fi
			cd /var/lib/jenkins/workspace/c-project-test
			make
		'''
            }
        }
    }
}
